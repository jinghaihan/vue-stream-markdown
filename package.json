{
  "name": "vue-stream-markdown",
  "type": "module",
  "version": "0.5.1",
  "packageManager": "pnpm@10.30.1",
  "description": "Streaming markdown output, Useful for text streams like LLM outputs.",
  "author": "jinghaihan",
  "license": "MIT",
  "homepage": "https://github.com/jinghaihan/vue-stream-markdown#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinghaihan/vue-stream-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/jinghaihan/vue-stream-markdown/issues"
  },
  "keywords": [
    "vue",
    "markdown",
    "stream",
    "mermaid",
    "katex",
    "latex",
    "customizable"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./index.css": "./dist/index.css",
    "./theme.css": "./dist/theme.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm playground:dev",
    "build": "tsdown",
    "playground:dev": "pnpm -F playground dev",
    "playground:build": "pnpm -F playground generate",
    "playground:typecheck": "pnpm -F playground typecheck",
    "playground:prepare": "pnpm -F playground prepare",
    "docs:dev": "pnpm -F docs dev",
    "docs:build": "pnpm -F docs build",
    "deps": "taze major -I",
    "lint": "eslint",
    "typecheck": "vue-tsc --noEmit && pnpm playground:typecheck",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "release": "bumpp",
    "prepare": "simple-git-hooks && pnpm playground:prepare",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "beautiful-mermaid": ">=0.1.0",
    "katex": ">=0.16.20",
    "mermaid": ">=11",
    "shiki": ">=3.15.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "beautiful-mermaid": {
      "optional": true
    },
    "katex": {
      "optional": true
    },
    "mermaid": {
      "optional": true
    },
    "shiki": {
      "optional": true
    }
  },
  "dependencies": {
    "@floating-ui/dom": "catalog:frontend",
    "@vueuse/core": "catalog:frontend",
    "marked": "catalog:parser",
    "mdast-util-from-markdown": "catalog:parser",
    "mdast-util-frontmatter": "catalog:parser",
    "mdast-util-gfm": "catalog:parser",
    "mdast-util-math": "catalog:parser",
    "mdast-util-to-markdown": "catalog:parser",
    "micromark-extension-cjk-friendly": "catalog:parser",
    "micromark-extension-cjk-friendly-gfm-strikethrough": "catalog:parser",
    "micromark-extension-frontmatter": "catalog:parser",
    "micromark-extension-gfm": "catalog:parser",
    "micromark-extension-math": "catalog:parser",
    "micromark-util-types": "catalog:parser"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "catalog:build",
    "tsdown": "catalog:build",
    "unplugin-icons": "catalog:build",
    "unplugin-lightningcss": "catalog:build",
    "vite": "catalog:build",
    "bumpp": "catalog:dev",
    "pncat": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:frontend",
    "@iconify-json/catppuccin": "catalog:icons",
    "@iconify-json/lucide": "catalog:icons",
    "@antfu/utils": "catalog:inlined",
    "quick-lru": "catalog:inlined",
    "treechop": "catalog:inlined",
    "@nuxt/eslint": "catalog:lint",
    "@octohash/eslint-config": "catalog:lint",
    "@unocss/eslint-plugin": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "lint-staged": "catalog:lint",
    "vue-tsc": "catalog:lint",
    "@vitest/coverage-v8": "catalog:test",
    "@vue/test-utils": "catalog:test",
    "vitest": "catalog:test",
    "@types/node": "catalog:types",
    "beautiful-mermaid": "^0.1.3",
    "katex": "^0.16.28",
    "mermaid": "^11.12.3",
    "shiki": "^3.22.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
